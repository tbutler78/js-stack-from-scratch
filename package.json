{
  "name": "your-project",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm dev",
    "dev": "nodemon --ignore lib --exec babel-node src/server",
    "prod-build": "rimraf lib && babel src -d lib --ignore .test.js",
    "prod-start":
      "cross-env NODE-ENV=production pm2 start lib/server && pm2 logs",
    "prod-stop": "pm2 delete server",
    "test": "eslint src && flow && jest --coverage",
    "precommit": "npm test",
    "prepush": "npm test && npm prod-build"
  },
  "browserslist": ["> 1%"],
  "dependencies": {
    "@types/compression": "0.0.35",
    "@types/express": "^4.11.0",
    "color": "^1.0.3",
    "compression": "^1.7.1",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.4.0",
    "@types/jest": "^22.0.1",
    "add": "^2.0.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.0.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "common-tags": "^1.6.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.62.0",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "jest-cli": "^22.0.4",
    "nodemon": "^1.14.7",
    "pm2": "^2.9.1",
    "rimraf": "^2.6.2"
  }
}
