{
  "name": "your-project",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev-start",
    "dev-start":
      "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev-wds": "webpack-dev-server --progress",
    "prod-build":
      "rimraf lib && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod-start":
      "cross-env NODE-ENV=production pm2 start lib/server && pm2 logs",
    "prod-stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "eslint src && flow && jest --coverage"
  },
  "browserslist": ["> 1%"],
  "dependencies": {
    "@types/compression": "0.0.35",
    "@types/express": "^4.11.0",
    "@types/react-hot-loader": "^3.0.5",
    "babel-polyfill": "^6.26.0",
    "color": "^1.0.3",
    "common-tags": "^1.6.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "react-hot-loader": "^4.0.0-beta.12"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/webpack": "^3.8.2",
    "@types/webpack-dev-server": "^2.9.2",
    "add": "^2.0.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-react-proptypes": "^13.0.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.62.0",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "jest-cli": "^22.0.4",
    "nodemon": "^1.14.7",
    "pm2": "^2.9.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
